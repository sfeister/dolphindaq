# Channel 1
record(int64out,"TEENSYPULSE:CH1:delay:set"){
    field(DESC, "Set pulse delay time")
	field(DTYP,"stream")
	field(DRVL,"0")
	field(DRVH,"10000000") # restrict users from setting overly high times
	field(EGU,"micros")
	field(OUT,"@teensypulse.proto DelayChannelN(1) teensypulse_ino")
}

